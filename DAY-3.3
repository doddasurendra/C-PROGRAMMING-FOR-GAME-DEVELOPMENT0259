#include <stdio.h>

void find_max_min(int nums[], int n, int *max, int *min);

int main() {
    int n, i, max, min;
    printf("Enter the number of integers: ");
    scanf("%d", &n);
    int nums[n];
    printf("Enter the integers: ");
    for (i = 0; i < n; i++) {
        scanf("%d", &nums[i]);
    }
    find_max_min(nums, n, &max, &min);
    printf("Maximum number is: %d\n", max);
    printf("Minimum number is: %d\n", min);
    return 0;
}

void find_max_min(int nums[], int n, int *max, int *min) {
    *max = nums[0];
    *min = nums[0];
    int i;
    for (i = 1; i < n; i++) {
        if (nums[i] > *max) {
            *max = nums[i];
        }
        if (nums[i] < *min) {
            *min = nums[i];
        }
    }
}
